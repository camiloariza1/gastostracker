{
    "family": "my-app-task-family",
    "containerDefinitions": [
        {
            "name": "gastostracker-container",
            "image": "851285784641.dkr.ecr.region.amazonaws.com/gastostracker-app:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "networkMode": "bridge",
    "executionRoleArn": "arn:aws:iam::851285784641:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["EC2"]
}